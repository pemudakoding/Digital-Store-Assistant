{
  "name": "koalastore-bot",
  "version": "2.0.0",
  "description": "WhatsApp Bot Store Assistant - Clean Version",
  "type": "module",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "start:interactive": "node start.js",
    "start:dev": "NODE_ENV=development node src/app.js",
    "dev": "NODE_ENV=development node src/app.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop koalastore-bot",
    "pm2:restart": "pm2 restart koalastore-bot",
    "pm2:reload": "pm2 reload koalastore-bot",
    "pm2:delete": "pm2 delete koalastore-bot",
    "pm2:logs": "pm2 logs koalastore-bot",
    "pm2:status": "pm2 status",
    "pm2:monit": "pm2 monit",
    "clean": "rm -rf sessionn && echo Session cleaned",
    "clean:win": "rmdir /s /q sessionn && echo Session cleaned",
    "kill": "taskkill /F /IM node.exe || echo No Node processes to kill",
    "logs:clear": "rm -rf logs/* && echo Logs cleared"
  },
  "keywords": [
    "whatsapp",
    "bot",
    "store",
    "assistant",
    "baileys"
  ],
  "author": "Koala Store",
  "license": "MIT",
  "dependencies": {
    "@whiskeysockets/baileys": "^6.7.18",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.12",
    "express": "^4.19.7",
    "figlet": "^1.8.0",
    "fluent-ffmpeg": "^2.1.3",
    "form-data": "^4.0.1",
    "jimp": "^0.16.1",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.46",
    "ms": "^2.1.3",
    "node-fetch": "^3.3.2",
    "node-upload-images": "^1.0.1",
    "node-webpmux": "^3.1.7",
    "p-queue": "^8.0.1",
    "parse-ms": "^4.0.0",
    "performance-now": "^2.1.0",
    "pino": "^9.5.0",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "qs": "^6.13.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
